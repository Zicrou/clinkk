<p id="notice"><%= notice %></p>

<h1>Caisses</h1>

<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th>Prenom</th>
      <th>Acte</th>
      <th>Payement</th>
      <th>Montant</th>
      <th>Telephone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @caisses.each do |caiss| %>
      <tr>
        <td><%= caiss.nom %></td>
        <td><%= caiss.prenom %></td>
        <td><%= caiss.acte %></td>
        <td><%= caiss.payement %></td>
        <td><%= caiss.montant %></td>
        <td><%= caiss.telephone %></td>
        <td><%= link_to 'Show', caiss %></td>
        <td><%= link_to 'Edit', edit_caiss_path(caiss) %></td>
        <td><%= link_to 'Destroy', caiss, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Caiss', new_caiss_path %>
